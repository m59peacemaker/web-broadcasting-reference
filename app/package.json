{
  "name": "app",
  "private": true,
  "version": "0.0.0",
  "description": "",
  "main": "index.js",
  "browserslist": [
    "last 2 versions",
    "safari >= 10",
    "ie >= 11"
  ],
  "scripts": {
    "build-js": "rollup -c rollup/dev.config.js",
    "build-css": "postcss src/app.css -o www/static/build/app.css",
    "build": "npm run build-js & npm run build-css & wait",
    "build-test": "rollup -c rollup/test.config.js",
    "clean": "rm -r ./www/static/build ./build",
    "test": "npm run --silent build-test | tape-run",
    "build-and-test": "sh -c 'npm run build & npm run test & wait'",
    "watch": "onchange -i 'src/**/*' -- npm run build-and-test",
    "server": "live-server --no-browser ./www",
    "dev": "sh -c 'npm run watch & npm run server & wait'"
  },
  "keywords": [],
  "author": "Johnny Hauser <johnnyhauser@gmail.com>",
  "license": "CC0-1.0",
  "devDependencies": {
    "live-server": "^1.2.0",
    "onchange": "^3.2.1",
    "postcss-cli": "^4.1.1",
    "postcss-define-property": "^0.5.0",
    "postcss-grid-kiss": "^1.4.1",
    "postcss-input-style": "^1.0.0",
    "postcss-nesting": "^4.2.1",
    "rollup": "^0.50.0",
    "rollup-plugin-alias": "^1.4.0",
    "rollup-plugin-buble": "^0.16.0",
    "rollup-plugin-commonjs": "^8.2.5",
    "rollup-plugin-multi-entry": "^2.0.2",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-svelte": "^3.1.0",
    "source-map-support": "^0.5.0",
    "tape-catch": "^1.0.6",
    "tape-run": "^3.0.0",
    "tape-universal": "^4.7.0",
    "zora": "^1.0.1"
  },
  "dependencies": {
    "addeventlistener": "^2.0.0",
    "deepmerge": "^2.0.1",
    "flyd": "^0.2.4",
    "flyd-droprepeats": "^2.0.1",
    "flyd-obj": "^0.1.0",
    "flyd-onanimationframe": "^0.3.0",
    "is-nil": "^1.0.1",
    "lightness": "^1.0.0",
    "mdi-es6": "^0.2.0",
    "nop": "^1.0.0",
    "normalizr": "^3.2.4",
    "ordered-entries": "^1.0.0",
    "pify": "^3.0.0",
    "purestate": "^1.0.0",
    "ramda": "^0.25.0",
    "svelte-extras": "^1.5.3",
    "svelte-modal": "^0.4.0",
    "svelte-transitions": "^1.1.1",
    "svelte-transitions-scale": "^1.0.0",
    "webrtc-adapter": "^5.0.6"
  }
}
